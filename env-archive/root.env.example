# Maula AI Platform - Production Environment
# Copy to .env and customize values

# Database
POSTGRES_USER=maula
POSTGRES_PASSWORD=maula_secure_password_change_this
POSTGRES_DB=maula

# JWT Secrets (generate unique values for production)
JWT_SECRET=your-super-secure-jwt-secret-at-least-32-characters
JWT_REFRESH_SECRET=your-super-secure-refresh-secret-at-least-32-characters

# AI API Keys
CEREBRAS_API_KEY=csk_your_cerebras_api_key_here
GROQ_API_KEY=gsk_your_groq_api_key_here
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# AWS (for S3 file storage - optional)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=ap-southeast-1
AWS_S3_BUCKET=maula-files

# Stripe (for billing - optional)
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
STRIPE_PRICE_PRO=

# Email Service (Google Workspace)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=info@maula.ai
SMTP_PASS=your-google-workspace-password
FROM_EMAIL=support@maula.ai
APP_URL=https://dyad.maula.ai

# GitHub OAuth (for Git integration - optional)
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# URLs
DYAD_URL=https://dyad.maula.ai
VSCS_URL=https://vscs.maula.ai
MAIN_URL=https://maula.ai
